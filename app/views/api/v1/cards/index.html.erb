<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Cards</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_cards.each do |api_v1_card| %>
      <tr>
        <td><%= link_to 'Show', api_v1_card %></td>
        <td><%= link_to 'Edit', edit_api_v1_card_path(api_v1_card) %></td>
        <td><%= link_to 'Destroy', api_v1_card, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Card', new_api_v1_card_path %>
